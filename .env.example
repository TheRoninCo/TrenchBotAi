# TrenchBotAi Environment Configuration
# Copy this to .env and fill in your actual values

# Helius API Configuration
HELIUS_API_KEY=your_helius_dev_api_key_here
HELIUS_RPC_URL=https://devnet.helius-rpc.com/
HELIUS_WEBHOOK_URL=https://api.helius.xyz/v0/webhooks

# Solana Network Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WS_URL=wss://api.devnet.solana.com

# Database Configuration
DATABASE_URL=postgres://user:password@localhost/trenchbot
REDIS_URL=redis://localhost:6379

# Telegram Bot (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Metrics and Monitoring
METRICS_ENABLED=true
GRAFANA_URL=http://localhost:3000
PROMETHEUS_URL=http://localhost:9090

# Trading Configuration
MAX_POSITION_SIZE_USD=1000
MIN_PROFIT_THRESHOLD_USD=10
GAS_PRICE_LIMIT_LAMPORTS=50000

# Risk Management
MAX_DAILY_LOSS_USD=500
MAX_CONCURRENT_TRADES=5
STOP_LOSS_PERCENTAGE=0.05

# AI/ML Configuration
MODEL_PATH=./models
ENABLE_GPU_ACCELERATION=false

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=info
RUST_BACKTRACE=1

# MEV Configuration
JITO_BLOCK_ENGINE_URL=https://mainnet.block-engine.jito.wtf
FLASHLOAN_PROVIDER=solend

# API Keys
SOLSCAN_API_KEY=your_solscan_api_key_here

# Jupiter API (free tier - no key required)
JUPITER_API_URL=https://quote-api.jup.ag/v6
JUPITER_PRICE_API_URL=https://price.jup.ag/v4/price

# Optional API Keys
BIRDEYE_API_KEY=your_birdeye_api_key_here